{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["index.js", "public/CloudflareCallsLibrary.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "opts": {
        "destination": "./public/docs",
        "recurse": true,
        "template": "node_modules/docdash",
        "readme": "./README.md"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}
