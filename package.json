{
  "name": "cloudflare-calls",
  "version": "0.1.4",
  "description": "A Reference implementation for Cloudflare Calls",
  "module": "public/CloudflareCalls.js",
  "browser": "public/CloudflareCalls.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "rollup": "rollup public/CloudflareCalls.js --file public/CloudflareCalls.min.js --format umd --name \"CloudflareCalls\"",
    "apidocs": "apidoc -i ./ -e \"(node_modules|public)\" -o public/docs/api",
    "docs": "jsdoc -c jsdoc.json",
    "build": "npm run docs && mkdir -p public/docs/api && npm run apidocs && npm run rollup",
    "start": "node ."
  },
  "author": "James Futhey",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^1.2.0",
    "docdash": "^2.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsdoc": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.6.7",
    "ws": "^8.18.0"
  }
}
